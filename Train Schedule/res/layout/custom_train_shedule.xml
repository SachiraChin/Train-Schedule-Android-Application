<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/custom_train_schedule_root" android:orientation="vertical"
	android:layout_width="fill_parent" android:layout_height="fill_parent">

	<TextView android:id="@+id/custom_train_schedule_train_name"
			  android:text="@+id/TextView01"
			  style="@style/custom_train_schedule_train_name"
			  android:layout_width="wrap_content"
			   android:layout_height="wrap_content">
	</TextView>
	
	<TextView android:id="@+id/custom_train_schedule_train_arrival"
			  android:text="@+id/TextView01"
			  android:layout_width="wrap_content"
			   android:layout_height="wrap_content">
	</TextView>
	
	<TextView android:id="@+id/custom_train_schedule_train_depature"
			  android:text="@+id/TextView01"
			  android:layout_width="wrap_content"
			   android:layout_height="wrap_content">
	</TextView>
	
	<TextView android:id="@+id/custom_train_schedule_train_arrival_destination"
			  android:text="@+id/TextView01"
			  android:layout_width="wrap_content"
			   android:layout_height="wrap_content">
	</TextView>
	
</LinearLayout>